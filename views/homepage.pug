doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
        link(rel="stylesheet", href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.6/dist/css/bootstrap.min.css")
    body   
        .container 
            h1 Custom object table
            a(href="/update-cobj?objectId=${object.id}") Add/Update this table
             
            table.table.mt-5
                thead
                    tr
                        th Name
                        th Bio
                        th Catchphrase
                tbody
                    each character in data
                        tr
                            td #{character.properties.name}
                            td #{character.properties.bio}
                            td #{character.properties.catchphrase}